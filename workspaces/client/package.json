{
  "name": "@user-clone/client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/google-map-react": "^2.1.0",
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/victory": "^33.1.4",
    "apollo": "^2.32.1",
    "autoprefixer": "^9.0.0",
    "google-map-react": "^2.1.9",
    "mock-apollo-client": "^1.0.0",
    "postcss": "^8.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.0.7",
    "react-hook-form": "^6.15.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "rimraf": "^3.0.2",
    "subscriptions-transport-ws": "^0.9.18",
    "tailwind": "^4.0.0",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.0.3",
    "victory": "^35.4.12",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "tailwind:build": "tailwind build ./src/styles/tailwind.css -o ./src/styles/styles.css",
    "start": "yarn run apollo:codegen & yarn run tailwind:build & react-scripts start",
    "apollo:codegen": "rimraf src/__generated__ && apollo client:codegen src/__generated__ --target=typescript --outputFlat",
    "build": "react-scripts build",
    "prebuild": "npm run tailwind:build",
    "test": "react-scripts test",
    "test:coverage": "yarn test -- --coverage --watchAl=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "eslint-plugin-react": "^7.22.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "./workspaces/client/src/components/**/*.tsx",
      "./workspaces/client/src/pages/**/*.tsx",
      "./workspaces/client/src/routers/**/*.tsx"
    ]
  }
}
